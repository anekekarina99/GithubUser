<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.consumerapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <permission
        android:name="com.android.dcdsubfunddua.READ_DATABASE"
        android:protectionLevel="normal" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DcdSubFundDua">
        <activity android:name=".ui.main.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.main.view.DetailActivity"
            android:theme="@style/Theme.DcdSubFundDua12" />
        <activity
            android:name=".ui.main.view.AboutActivity"
            android:theme="@style/Theme.DcdSubFundDua12" />

        <activity
            android:name=".ui.main.view.AlarmActivity"
            android:theme="@style/Theme.DcdSubFundDua12" />
        <activity
            android:name=".ui.main.view.FavorActivity"
            android:theme="@style/Theme.DcdSubFundDua12" />
        <activity
            android:name=".ui.main.view.KontakActivity"
            android:theme="@style/Theme.DcdSubFundDua12" />


    </application>

    <queries>
        <package android:name="com.android.consumerapp" />
    </queries>

</manifest>